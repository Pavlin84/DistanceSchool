@model DistanceSchool.Web.ViewModels.Managers.SchoolManagerHomeViewModel

@{
    this.ViewData["Title"] = "SchoolManager";
}

<h1 class="text-md-center border-dark">Училище : @Model.SchoolName</h1>
<h3 class="text-md-center border-dark">Директор : @Model.SchoolManager</h3>
<div class="row">
    <div class="col-md-5 rounded container-fluid" style="background-color:aquamarine">
        <h4 class="text-md-center">Кандидати за преподавател :</h4>
        <partial name="_CandidacesList" model="@Model.Candidacies" />
    </div>
</div>
@section PrivateSideBar{
    <li class="nav-item">
        <a href="/Administration/Dashboard/StudentCandidacies?SchoolId=@Model.SchoolId" class="nav-link text-dark">Заявления за обучение</a>
    </li>
}